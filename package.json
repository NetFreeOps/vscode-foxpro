{
	"name": "foxpro",
	"displayName": "FoxPro",
	"description": "Visual FoxPro",
	"version": "1.3.0",
	"publisher": "TomBellin",
	"engines": {
		"vscode": "^1.8.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"configurationDefaults": {
			"[foxpro]": {
				"files.encoding": "windows1252"
			}
		},
  		"languages": [{
			"id": "foxpro",
			"aliases": ["foxpro", "Visual FoxPro", "VFP"],
			"extensions": [".prg", ".PRG"],
			"configuration": "./foxpro.configuration.json"
		}],
		"snippets": [
			{
				"language": "foxpro",
				"path": "./snippets/foxpro_snippets.json"
			}
		],
		"grammars": [{
			"language": "foxpro",
			"scopeName": ".foxpro",
			"path": "./syntaxes/foxpro.plist"
		}]
	}
}